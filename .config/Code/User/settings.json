{
  "i18n-ally.localesPaths": "src/locales",
  "i18n-ally.keystyle": "nested",
  "editor.fontFamily": "'Hack', 'monospace', monospace, 'Hack'",
  "vim.smartRelativeLine": false,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "~/.config/nvim/init.vim",
  "vim.normalModeKeyBindings": [{
    "before": [";"],
    "after": [":"]
  }],
  "vim.insertModeKeyBindings": [
    {
        "before": ["j", "j"],
        "after": ["<esc>"]
    },
    { "before": ["<C-h>"],
      "after": ["<Left>"]
    },
    {

    "before": ["<C-j>"],
    "after": ["<Down>"]
    },
    {
    "before": ["<C-k>"],
    "after": ["<Up>"]
    },
    {
    "before": ["<C-l>"],
    "after": ["<Right>"]
    }
  ],
  "editor.lineNumbers": "relative",
  "zenMode.fullScreen": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.hideTabs": false,
  "zenMode.hideActivityBar": false,
  "workbench.editor.closeEmptyGroups": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "vetur.completion.autoImport": true,
  "eslint.validate": ["javascript", "vue"],
  "commentAnchors.workspace.excludeFiles": "**/{node_modules,dist,.git,.idea,target,out,build,vendor}/**/*",
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": true,
      "compare": true,
      "history": false,
      "remote": false
    },
    "editorGroup": {
      "blame": true,
      "compare": true
    },
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scm": {
      "authors": true
    },
    "scmGroupInline": {
      "stash": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": false,
      "stash": true
    }
  },
  "workbench.colorTheme": "Gruvbox Dark Medium",
  "workbench.iconTheme": "material-icon-theme",
  "tabnine.experimentalAutoImports": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "editor.minimap.enabled": false,
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.linux": "/home/dubai/.config/nvim/init.vim",
  "python.linting.mypyEnabled": true,
  "python.linting.enabled": true,
  "explorer.confirmDelete": false
}

